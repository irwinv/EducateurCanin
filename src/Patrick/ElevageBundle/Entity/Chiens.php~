<?php

namespace Patrick\ElevageBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Patrick\ElevageBundle\Entity\Chiens
 *
 * @ORM\Table()
 * @ORM\Entity
 */
class Chiens
{
    /**
     * @var integer $id
     *
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;

    /**
     * @var string $nom
     *
     * @ORM\Column(name="nom", type="string", length=50)
     */
    private $nom;

    /**
     * @var varchar $sexe
     *
     * @ORM\Column(name="sexe", type="string", length="1")
     */
    private $sexe;

    /**
     * @var integer $chiot
     *
     * @ORM\Column(name="chiot", type="integer")
     */
    private $chiot;

    /**
     * @var string $titre
     *
     * @ORM\Column(name="titre", type="string", length=100)
     */
    private $titre;

    /**
     * @var integer $race
     *
     * @ORM\Column(name="race", type="integer")
     */
    private $race;

	/**
	* @ORM\OneToMany(targetEntity="Images", mappedBy="chien", cascade={"all"})
	*
	*/
	private $img;
}